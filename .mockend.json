{
  "Category": {
    "title": { "string": {} },
    "products": {
      "hasMany": "Product"
    }
  },
  "Product": {
    "code": { 
      "regexp": "[A-Z0-9]{5}" 
    },
    "name": { "string": {} },
    "active": { 
      "boolean": {} 
    },
    "description": { "string": {} },
    "fispq_url": { 
      "regexp": "https?://(www.)?[-a-zA-Z0-9@:%._+~#=]{1,256}.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_+.~#?&//=]*)"
    },
    "barcode": {
       "int": {
        "min": 1111111111,
        "max": 9999999999
       } },
    "qrcode": { 
      "int": {
        "min": 111111111111111,
        "max": 111111111111111
      } },
    "category": {
      "belongsTo": "Category"
    }
  }
}